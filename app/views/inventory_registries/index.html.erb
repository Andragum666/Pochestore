<p id="notice"><%= notice %></p>

<h1>Inventory Registries</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Inventory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventory_registries.each do |inventory_registry| %>
      <tr>
        <td><%= inventory_registry.user %></td>
        <td><%= inventory_registry.inventory %></td>
        <td><%= link_to 'Show', inventory_registry %></td>
        <td><%= link_to 'Edit', edit_inventory_registry_path(inventory_registry) %></td>
        <td><%= link_to 'Destroy', inventory_registry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inventory Registry', new_inventory_registry_path %>
